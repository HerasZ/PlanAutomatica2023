(defproblem problem servicioemergencia
    (
    (dron d1)(brazo brazod)(brazo brazoi)(brazos-dron d1 brazod)(brazos-dron d1 brazoi)(libre-brazo d1 brazod)(libre-brazo d1 brazoi)(distintos brazoi brazod)
    (contenedor c1)(loc-contenedor base c1)
    (contenedor c2)(loc-contenedor base c2)
    (capacidad-contenedor c1 0)
    (capacidad-contenedor c2 0)
    (capacidad-maxima c1 5)
    (capacidad-maxima c2 6)
    (contenido medicina)
    (contenido comida)
    (base base)
    (loc base)(cajas-base medicina 9)(cajas-base comida 4)
    (loc l2)(cajas-localizacion medicina l2 0)(cajas-localizacion comida l2 0)
    (loc l3)(cajas-localizacion medicina l3 0)(cajas-localizacion comida l3 0)
    (loc-dron base d1)
    (necesita-cajas medicina l2 6) 
    (necesita-cajas comida l3 3)
    
    )
    ((enviar-todo d1))
)
